{
  "arrowParens": "always",
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.yml",
      "options": {
        "tabWidth": 2,
        "useTabs": false
      }
    },
    {
      "files": ".travis.yml",
      "options": {
        "tabWidth": 2,
        "useTabs": false
      }
    }
  ]
}
